      <th>Sales tax total</th>
      <th>Total amount</th>
      <th>Currency</th>
      <th>Exchange rate</th>
      <th>Total amount in home currency</th>
      <th>Memo</th>
      <th>Customer msg</th>
      <th>Is to be emailed</th>
      <th>Customer sales tax code</th>
      <th>Other</th>
      <th>External guid</th>
      <th>Linked txn</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @estimates.each do |estimate| %>
      <tr>
        <td><%= estimate.id %></td>
        <td><%= estimate.time_created %></td>
        <td><%= estimate.time_modified %></td>
        <td><%= estimate.edit_sequence %></td>
        <td><%= estimate.txn_number %></td>
        <td><%= estimate.customer_id %></td>
        <td><%= estimate.class_id %></td>
        <td><%= estimate.template_id %></td>
        <td><%= estimate.txn_date %></td>
        <td><%= estimate.ref_number %></td>
        <td><%= estimate.bill_address_addr1 %></td>
        <td><%= estimate.bill_address_addr2 %></td>
        <td><%= estimate.bill_address_addr3 %></td>
        <td><%= estimate.bill_address_addr4 %></td>
        <td><%= estimate.bill_address_addr5 %></td>
        <td><%= estimate.bill_address_city %></td>
        <td><%= estimate.bill_address_state %></td>
        <td><%= estimate.bill_address_postal_code %></td>
        <td><%= estimate.bill_address_country %></td>
        <td><%= estimate.bill_address_note %></td>
        <td><%= estimate.ship_address_addr1 %></td>
        <td><%= estimate.ship_address_addr2 %></td>
        <td><%= estimate.ship_address_addr3 %></td>
        <td><%= estimate.ship_address_addr4 %></td>
        <td><%= estimate.ship_address_addr5 %></td>
        <td><%= estimate.ship_address_city %></td>
        <td><%= estimate.ship_address_state %></td>
        <td><%= estimate.ship_address_postal_code %></td>
        <td><%= estimate.ship_address_country %></td>
        <td><%= estimate.ship_address_note %></td>
        <td><%= estimate.is_active %></td>
        <td><%= estimate.po_number %></td>
        <td><%= estimate.terms_id %></td>
        <td><%= estimate.due_date %></td>
        <td><%= estimate.sales_rep_id %></td>
        <td><%= estimate.fob %></td>
        <td><%= estimate.subtotal %></td>
        <td><%= estimate.item_sales_tax_id %></td>
        <td><%= estimate.sales_tax_percentage %></td>
        <td><%= estimate.sales_tax_total %></td>
        <td><%= estimate.total_amount %></td>
        <td><%= estimate.currency_id %></td>
        <td><%= estimate.exchange_rate %></td>
        <td><%= estimate.total_amount_in_home_currency %></td>
        <td><%= estimate.memo %></td>
        <td><%= estimate.customer_msg_id %></td>
        <td><%= estimate.is_to_be_emailed %></td>
        <td><%= estimate.customer_sales_tax_code_id %></td>
        <td><%= estimate.other %></td>
        <td><%= estimate.external_guid %></td>
        <td><%= estimate.linked_txn_id %></td>
        <td><%= link_to 'Show', estimate %></td>
      </tr>
    <% end %>
  </tbody>
</table>

