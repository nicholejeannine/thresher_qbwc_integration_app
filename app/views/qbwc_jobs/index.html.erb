<div class="page-header">
  <h1 class="text-center">Pending Jobs</h1>
</div>

<table class="table table-striped table-bordered table-condensed">
  <thead>
  <tr>
    <th>Name</th>
    <th>Enabled</th>
    <th>Worker</th>
    <th>Requests</th>
    <th>Data</th>
    <th>Last Ran</th>
  </tr>
  </thead>
  <tbody>
  <% @jobs.each do |job| %>
<tr>
  <td> <%= job.name %></td>
  <td> <%= job.enabled %></td>
  <td> <%= job.worker_class %></td>
  <td> <%= job.requests %></td>
  <td> <%= job.data %></td>
  <td> <%= job.updated_at.to_time.to_formatted_s(:db) %></td>
</tr>
<% end %>
  </tbody>
</table>