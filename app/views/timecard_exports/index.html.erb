<div class="page-header">
  <h1 class="text-center">Time card Exports</h1>
</div>

<table class="table table-striped table-bordered table-condensed">
  <thead>
  <tr>
    <th>Date Range</th>
    <th>Date Exported</th>
  </tr>
  </thead>
  <tbody>
  <% @exports.each do |export| %>
      <tr>
        <td> <%= export.yearweek %></td>
        <td> <%= export.exported_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<%= link_to "New Export", create_export_path, method: "POST", class: "btn btn-info pull-right margin-bottom", remote: true %>
