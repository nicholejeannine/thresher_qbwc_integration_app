<%- headers = ['Employee Name', 'Customer/Job', 'Notes', 'Duration', 'Date'] -%>
<%= CSV.generate_line headers %>
<%- @entries.each do |entry| -%>
  <%- row = [entry.employee.full_name, entry.customer_full_name, entry.qb_notes, entry.duration, entry.tc_date.to_formatted_s(:db)] -%>
  <%= raw CSV.generate_line row -%>
<%- end -%>