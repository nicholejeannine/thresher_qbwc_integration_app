<div class="row">
  <div class="col-lg-12">
    <h1 class="text-center">Pending Exports</h1>
  </div>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
  <tr>
    <th>Employee Name</th>
    <th>Customer/Job</th>
    <th>Notes</th>
    <th>Duration</th>
    <th>Date</th>
    <th>Status</th>
  </tr>
  </thead>
  <tbody>
<% @timecards.each do |t| %>
      <tr>
        <td> <%= t.qb_employee.name %></td>
        <td> <%= t.lookup_customer_name %></td>
        <td> <%= t.qb_notes %></td>
        <td> <%= t.duration %></td>
        <td> <%= t.tc_date.to_formatted_s(:db) %></td>
        <td> <%= t.tc_status %></td>
      </tr>
  <% end %>
  </tbody>
</table>